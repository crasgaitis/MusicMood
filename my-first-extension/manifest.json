{
  "version": "0.1",
  "manifest_version": 2,
  "name": "spooky spook",
  "author": "Tal Wolman",
  "description": "my description",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts" : [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "32": "icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "disco"
  },
  "chrome_url_overrides": {
    "newtab": "a-3/index.html"
  }
 }